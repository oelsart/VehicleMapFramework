							<li>
								<def>FuelConsumptionRate</def>
								<value>8</value>
							</li>
						</statCategories>
					</li>
				</upgrades>
			</li>
		</comps>
		<placeWorkers>
			<li>VehicleInteriors.PlaceWorker_ForbidOnHumanPoweredVehicle</li>
		</placeWorkers>
	</ThingDef>

